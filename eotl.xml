    <AliasPackage>
        <AliasGroup isActive="yes" isFolder="yes">
            <name>mudlet-eotl</name>
            <script></script>
            <command></command>
            <packageName></packageName>
            <regex></regex>
            <Alias isActive="yes" isFolder="no">
                <name>ui chatbox mute</name>
                <script>if true == mute then
	echo( &quot;Unmuted\n&quot; )
	mute = false
else
	echo( &quot;Muted\n&quot; )
	mute = true
end</script>
                <command></command>
                <packageName></packageName>
                <regex>^#mute$</regex>
            </Alias>
            <Alias isActive="yes" isFolder="no">
                <name>follow helper</name>
                <script>follow_who = matches[2]
if follow_who then
	follow_who = string.gsub( follow_who, &quot;%s+&quot;, &quot;&quot; )
end

if follow_who or true == follow_helper then
	if false == follow_helper then
		echo( &quot;Follow Helper: ON\n&quot; )
		echo( &quot;You are now following &quot; .. follow_who ..&quot;\n&quot; )
		follow_helper = true
	else
		echo( &quot;Follow Helper: OFF\n&quot; )
		follow_helper = false
		follow_who = &quot;&quot;
	end
else
	echo( &quot;Follow who?  Example: #follow draevokk\n&quot; )
end</script>
                <command></command>
                <packageName></packageName>
                <regex>^#follow( .+)?$</regex>
            </Alias>
            <Alias isActive="yes" isFolder="no">
                <name>go</name>
                <script>eotl_go()</script>
                <command></command>
                <packageName></packageName>
                <regex>^#go (back )?(.+)$</regex>
            </Alias>
            <Alias isActive="yes" isFolder="no">
                <name>go and rest</name>
                <script>matches[2] = &quot;&quot;
eotl_go()
send( &quot;rest&quot; )</script>
                <command></command>
                <packageName></packageName>
                <regex>^#rgo (back )?(.+)$</regex>
            </Alias>
            <Alias isActive="yes" isFolder="no">
                <name>leading helper</name>
                <script>lead_who = matches[2]
if lead_who then
	lead_who = string.gsub( lead_who, &quot;%s+&quot;, &quot;&quot; )
end

if lead_who or true == leading_helper then
	if false == leading_helper then
		echo( &quot;Leading Helper: ON\n&quot; )
		echo( &quot;You are now leading &quot; .. lead_who ..&quot;\n&quot; )
		leading_helper = true
	else
		echo( &quot;Leading Helper: OFF\n&quot; )
		leading_helper = false
		lead_who = &quot;&quot;
	end
else
	echo( &quot;Lead who?  Example: #lead draevokk\n&quot; )
end</script>
                <command></command>
                <packageName></packageName>
                <regex>^#lead( .+)?$</regex>
            </Alias>
